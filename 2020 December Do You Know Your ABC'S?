import java.util.*;

public class DoYouKnowYourABCs {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int[] numbers = new int[7];

        for(int i = 0; i < 7; i++) {
            numbers[i] = scan.nextInt();
        }

        Arrays.sort(numbers);

        int A = numbers[0];
        int B = numbers[1];
        int C = numbers[6] - A - B;

        System.out.println(A + " " + B + " " + C);
    }
}
